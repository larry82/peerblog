<%= simple_form_for(@quiz_result) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :quiz %>
    <%= f.input :scores %>
    <%= f.input :answers %>
    <%= f.input :email %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
